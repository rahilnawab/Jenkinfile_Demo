    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building from github...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing from github..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying from github....'
             }
        }
    	stage('Release') {
                steps {
                echo 'Released....'
            }
        }
    }
}
